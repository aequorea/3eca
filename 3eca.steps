3eca steps
----------

download 3eca.pdb from rcsb.org
save as 3ecaA.pdb
type findSurfaceResidues
save atoms as 3ecaA_exposed_atm.pdb
save residues as 3ecaA_exposed_res.pdb
select helix, ss H
save as 3ecaA_helix.pdb
select sheet, ss S
save as 3ecaA_sheet.pdb

